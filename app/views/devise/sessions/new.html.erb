<%# <h2>Log in</h2> %>
<div class="knowAbout">
    <img class="image" src="https://static.xx.fbcdn.net/rsrc.php/y8/r/dF5SId3UHWd.svg" alt="facebook">
    <h2 class="paraToKnow"> Connect with friends and the world around you on Facebook. </h2>
</div>

<%= form_for(resource, as: resource_name, html: {'data-turbo' => "false"}, url: session_path(resource_name)) do |f| %>

<body style="background-color: #F4F4F2;">

<div class="form1">

  <div class="field1">
    <%= f.email_field :email, placeholder:"Email" , class: "classy1" , autofocus: true, autocomplete: "email" %>
  </div>

  <div class="field1">
    <%= f.password_field :password, placeholder: "Password", class: "classy1" , autocomplete: "current-password" %>
  </div>

  <div class="actions1">
    <%= f.submit "Log in", class: "SignIn-btn1" %>
  </div>

  <a class="forg"><%= link_to 'Forget password?', new_user_password_path %></a>
  
  <div class="lineBreak1"></div>
  
  <div class="actions1">
     <%= link_to "Sign up", new_user_registration_path, class: "SignUp-btn1" %>
  </div>
<% end %>
</form>
<div>
  <footer class="Sign-in-fo">
      <p><a href="#"> English (UK) </a> | <a href="#"> العربية</a> </p>
      <br>
      <p id="ourTeam">© 2022 Tech Phantoms</p><br>
  </footer>
</body>